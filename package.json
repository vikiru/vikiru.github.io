{
	"name": "portfolio-website",
	"version": "3.2.0",
	"description": "Personal portfolio website showcasing my projects and skills. Built using React, React Router and TailwindCSS.",
	"private": true,
	"author": "Visakan Kirubakaran",
	"license": "MIT",
	"homepage": "https://vikiru.vercel.app",
	"repository": {
		"type": "git",
		"url": "https://github.com/vikiru/vikiru.github.io"
	},
	"keywords": [
		"Visakan",
		"Kirubakaran",
		"portfolio",
		"website",
		"react",
		"react router",
		"tailwindcss",
		"software",
		"developer"
	],
	"bugs": {
		"url": "https://github.com/vikiru/vikiru.github.io/issues"
	},
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"prepare": "husky",
		"lint": "biome lint",
		"format": "biome check --write",
		"generate-sitemap": "ts-node -r module-alias/register src/scripts/generateSitemap.ts",
		"typecheck": "tsc --noEmit"
	},
	"browserslist": {
		"production": [
			"defaults",
			">0.01%",
			"not dead",
			"not op_mini all",
			"last 2 chrome version",
			"last 2 firefox version",
			"last 2 safari version"
		],
		"development": [
			"defaults",
			">0.01%",
			"not dead",
			"not op_mini all",
			"last 2 chrome version",
			"last 2 firefox version",
			"last 2 safari version"
		]
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/react-fontawesome": "^0.2.6",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^14.3.1",
		"@testing-library/user-event": "^14.6.1",
		"customize-cra": "^1.0.0",
		"kwesforms": "^2.2.9",
		"react": "^18.3.1",
		"react-app-rewired": "^2.2.1",
		"react-dom": "^18.3.1",
		"react-helmet": "^6.1.0",
		"react-router-dom": "^6.30.2",
		"typescript": "^5.9.3",
		"web-vitals": "^3.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.28.5",
		"@babel/parser": "^7.28.5",
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@babel/plugin-transform-private-property-in-object": "^7.27.1",
		"@biomejs/biome": "2.3.7",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@semantic-release/git": "^10.0.1",
		"@tailwindcss/typography": "^0.5.19",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@types/react-router-dom": "^5.3.3",
		"autoprefixer": "^10.4.22",
		"gh-pages": "^6.3.0",
		"globals": "^15.15.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.2",
		"module-alias": "^2.2.3",
		"moment": "^2.30.1",
		"postcss": "^8.5.6",
		"postcss-import": "^16.1.1",
		"semantic-release": "^24.2.9",
		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.18",
		"ts-node": "^10.9.2"
	},
	"_moduleAliases": {
		"~assets": "src/assets",
		"~components": "src/components",
		"~data": "src/assets/data",
		"~hooks": "src/hooks",
		"~pages": "src/pages",
		"~scripts": "src/scripts",
		"~types": "src/types"
	},
	"lint-staged": {
		"*.{js,mjs,cjs,ts,jsx,tsx}": [
			"pnpm lint"
		]
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/npm",
			"@semantic-release/git",
			"@semantic-release/github"
		]
	},
	"overrides": {
		"serialize-javascript": "^6.0.2",
		"nth-check": "^2.1.1",
		"postcss": "^8.4.31"
	}
}
